<div class="page_row" id="travelsavers_processing_page_content">
	<div class="page_column_12 first rounded_corners">


	<%= stylesheet_link_tag "/themes/travelsavers/stylesheets/travelsavers_processing_page.css" %>

	<!-- Overlay to block clicking on the page while processing -->
	<div id="freeze_overlay" style="background: black; filter:alpha(opacity=1); opacity: .01; 
	position: fixed; top: 0; left: 0; width: 100%; height: 100%; z-index: 9999;"></div>

	<div id="travelsavers_processing_page_content">
	    <div id="processing_message" try-to-resolve-url="<%= try_to_resolve_travelsavers_booking_url(@travelsavers_booking) %>">
	        <img src="/themes/daily_deals/images/processing.gif">
	        <p>Please wait while your purchase is processed.</p>
	    </div>
	    <div class="push"></div>
	</div>

	<!-- <div id="travelsavers_processing_page_footer">
	    <%#= raw render :partial => 'shared/a_a_alt' %>
	</div> -->

	</div>
</div>
  <title>{% daily_deal_page_title %}</title>
{% include "shared/tracking_google_analytics" %}
</head>

<body>

<div class="clear"></div>
<!--Analog_Analytics_Daily_Deal-->

{% include with_theme "shared/header" %}

<a id="feedbacktab" target="_blank" href="https://www.research.net/s/stvdailydealsfeedback"><span>Leave your feedback</span></a>


<div id="analog_analytics">
	<div id="daily_deal">
	
		<div id="banner">
			<div class="clear"></div>
			
			<div id="market">
			   <div id="text">Choose Your City:</div>
			   <!-- market dropdown -->
			   <div id="market_menu_wrapper">
				<ul id="market_menu">
				  <li><a href="#">{{ publisher.market_name_or_city }}</a>
						  {{ publisher | market_selection_list }}
				  </li>
				</ul>
			   </div><!-- /market_menu_wrapper -->
			   <div class="clear"></div>              
			   <!--/ market dropdown -->
		    </div>
			
			{% unless publisher.current_user_belongs_to? %}
			<div id="subscribe_form">
				<h4 class="subscribe">SIGN UP FOR FREE DEAL ALERTS!</h4>
				<form action="{{ publisher.subscribers_path }}" id="daily_deal_header" class="email_subscription_tracking" method="post">
					<p><input name="subscriber[must_accept_terms]" value="true" type="hidden" /></p>
					<p><input name="redirect_to" value="{{ publisher.todays_daily_deal_path | verifiable_url }}" type="hidden" /></p>
					<p><input id="subscriber_email" name="subscriber[email]" size="30" type="text" value="Email Address" onclick="this.value='';" onfocus="this.select()" onblur="this.value=!this.value?'Email Address':this.value;" /></p>
					<p><input id="subscribe_button" type="submit" value="Sign Up" data-aa-tracked="true" data-aa-tracking-id="email-signup-thankyou"/></p>
					<div><p id="terms"><input name="subscriber[terms]" value="0" type="hidden" />
						<input id="subscriber_terms" name="subscriber[terms]" value="1" type="checkbox" /> I agree to the &nbsp;<a href="#" onclick="window.open('{{ publisher.terms_path }}', '_blank'); return false; return false;">Terms</a> and &nbsp;<a href="#" onclick="window.open('{{ publisher.privacy_path }}', '_blank'); return false; return false;">Privacy Policy</a>
					</p></div>
				</form>
				<p class="smalltext">By signing up for STV's Daily Deals I agree to receive emails from STV about its products &amp; services.</p>

			</div><!-- /subscribe_form -->
			{% endunless %}
		</div><!--banner-->   
		 
		<div id="dd_content">
			{% include with_theme "shared/dd_nav" %}
	       	 
		    {% include "shared/flash" %}
		    {{ content_for_layout }}
		    <div class="clear"></div>
		    
		</div><!--dd_content-->
		
		<div id="dd_footer">
			{% include "shared/a_a_alt" %}
			<ul id="footer_nav">
				<li><a href="{{ publisher.todays_daily_deal_path }}">Today's Deal</a> </li>
				<li><a href="{{ publisher.how_it_works_path }}">How It Works</a></li>
				<!-- <li><a href="">Past Deals</a></li> -->
				<li><a href="{{ publisher.feature_your_business_path }}">Feature Your Business</a></li>
				<li><a href="{{ publisher.faq_path }}">FAQ</a></li>
				<li class="last"><a href="{{ publisher.contact_path }}">Contact Us</a></li>
			</ul><!-- /footer_nav -->
			<div class="clear"></div>
			<ul id="legal_nav">
				<li><a href="{{ publisher.privacy_path }}">Privacy Policy</a></li>
				<li class="last"><a href="{{ publisher.terms_path }}">Terms and Conditions</a></li>
			</ul><!-- /legal_nav -->
		</div><!-- /dd_footer -->
	</div><!--daily_deal-->

</div><!--analog_analytics-->
{% include "daily_deals/javascript" %}
{% include with_theme "shared/tracking_publisher" %}

<script type="text/javascript" charset="utf-8">
	jQuery(document).find('*[data-aa-tracked]').each(function(){
	   jQuery(this).bind('click', function(event){
			switch (jQuery(this).attr('data-aa-tracking-id')){
				case 'email-signup-thankyou': jQuery.ajax({ url: '/themes/stv/javascripts/email-signup-thankyou.js', async: false});
				  break;
				case 'friend-referral-thankyou': jQuery.ajax({ url:'/themes/stv/javascripts/friend-referral-thankyou.js', async: false}); 
				  break;

			}
		});
	});
</script>
</body>
</html>

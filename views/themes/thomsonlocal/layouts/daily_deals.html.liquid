		          <p><input name="redirect_to" value="{{ publisher.todays_daily_deal_path}}" type="hidden" /></p>
		          <p><input id="subscriber_email" name="subscriber[email]" size="30" type="text" value="Email Address" onclick="this.value='';" /></p>

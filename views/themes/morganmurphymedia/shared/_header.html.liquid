<head>
 
<title>{% daily_deal_page_title %}</title> 

<link rel="stylesheet" href="/stylesheets/application.css" type="text/css" />
<link rel="stylesheet" href="/stylesheets/scoped_reset.css" type="text/css" />
<link rel="stylesheet" href="/themes/stylesheets/daily_deals.css" type="text/css" />
<!--<link rel="stylesheet" href="/themes/morganmurphymedia/stylesheets/customized_styles.css" type="text/css" /> THIS INCLUDE IS ONLY NECESSARY FOR PUBLISHERS WITHIN PUBLISHER GROUPS-->
<link href="/themes/morganmurphymedia/stylesheets/daily_deals.css" media="all" rel="stylesheet" type="text/css" />

<!--[if gt IE 6]>
<link href="/stylesheets/ie.css" rel="stylesheet" type="text/css" />
<![endif]-->

<script src="/javascripts/lib/prototype.js" type="text/javascript"></script>
<script src="/javascripts/effects.js" type="text/javascript"></script>
<script src="/javascripts/dragdrop.js" type="text/javascript"></script>
<script src="/javascripts/controls.js" type="text/javascript"></script>
<script src="/javascripts/application.js" type="text/javascript"></script>


{% include "shared/tracking_google_analytics" %}
</head>

<body>


<div id="analog_analytics">
    <div id="daily_deal" class="presignup">
		
		<div id="topbanner">
		  <img src="/themes/morganmurphymediaspokane/images/header_new.jpg" width="1000" height="150" alt="BEST DEALS MADISON AND BEYOND" />
		</div>
		
		<div id="topnavbar">
			<div id="nav">
				<ul>
					<li class="selected"><a href="#">Today's Deal</a> &nbsp;&nbsp;|&nbsp;&nbsp; </li>
					<li><a href="{{ publisher.how_it_works_path }}">How It Works</a> &nbsp;&nbsp;|&nbsp;&nbsp; </li>
					<li><a href="{{ publisher.faq_path }}">FAQ</a> &nbsp;&nbsp;|&nbsp;&nbsp; </li>
					<li><a href="{{ publisher.feature_your_business_path }}">Feature Your Business</a> &nbsp;&nbsp;|&nbsp;&nbsp; </li>
					<li><a href="{{ publisher.contact_path }}">Contact Us</a> &nbsp;&nbsp;|&nbsp;&nbsp; </li>
					<li><a href="{{ publisher.login_path }}">Sign In</a></li>					
				</ul>
			</div>
			<div id="subscribe_form">
				<form action="{{ publisher.subscribers_path }}" id="daily_deal_header" class="email_subscription_tracking" method="post">
					<p><input id="subscriber_must_accept_terms" name="subscriber[must_accept_terms]" type="hidden" value="true" /></p>
					<p><input name="redirect_to" value="{{ publisher.todays_daily_deal_path | verifiable_url }}" type="hidden" /></p>
					<p><input id="subscriber_email" name="subscriber[email]" size="30" type="text" value="Email Address" onclick="this.value='';" /></p>
					<p><input id="subscribe_button" type="submit" value="sign up" /></p>
					<p id="terms"><input name="subscriber[terms]" value="0" type="hidden" />
						<input id="subscriber_terms" name="subscriber[terms]" value="1" type="checkbox" /> I agree to the <a href="#" onclick="window.open('{{ publisher.terms_path }}', '_blank'); return false; return false;">Terms</a> and <a href="#" onclick="window.open('{{ publisher.privacy_path }}', '_blank'); return false; return false;">Privacy Policy</a>
					</p>
				</form>
			</div>
		</div>

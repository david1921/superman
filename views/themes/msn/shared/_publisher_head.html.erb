<script type="text/javascript">
//patch for top navigation and Bing Search Field
jQuery(document).ready(function($) {
	var a = $(".nav3");
	if ($.isDefined(a)) a.menuNavBar({
		delay: {
			open: 100,
			close: 150
		},
		stickyMenu: true
	});
	var titleVal = $("#q").attr('title');
	$("#q").val(titleVal);
	$("#q").click(function() {
	  var thisvar = $(this);
    var originalvalue = thisvar.attr('title');
    var originalcss = thisvar.css('color');
    thisvar.addClass('input-focus');   
    
    if (thisvar.val() == originalvalue) {
    		thisvar.val('').css({
    			"background" : "#ffffff"
    			});
    }    
    thisvar.blur(function() {
        if (thisvar.val() == '') {
          thisvar.val(originalvalue).removeClass('input-focus').css({
          	'color' : originalcss,
          	'background' : 'url("/themes/msn/publisher/images/binglogo.gif") no-repeat scroll right top transparent'
          });
        }
    });
	});	
	$(".higmore").hover(function(){
			$("#more").css({"display" : "block"});
	});
	$("#more").mouseleave(function(){
			$(this).css({"display" : "none"});
	});
	$(".opts a").hover(function(){
			$("#pgoptslinks").css({"display" : "block"});
	});
	$("#pgoptslinks").mouseleave(function(){
			$(this).css({"display" : "none"});
	});
});
</script>

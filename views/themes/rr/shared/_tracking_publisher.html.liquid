<script src="/themes/rr/javascripts/s_code.js" type="text/javascript"></script>
<script type="text/javascript">

//*** TWC Omniture Variables ***/
s.products = "{{ daily_deal.id }}";

if (/\/daily_deal_purchases\/new$/.test(document.location.pathname)) {
  s.events ="scAdd"
} else if (/\/daily_deal_purchases\/.*?\/confirm$/.test(document.location.pathname)) {
  s.events ="scCheckout"
} else if (/(\/daily_deal\/login)|(\/consumers\/new)$/.test(document.location.pathname)) {
  s.events = "event4";
} else if (/\/consumers$/.test(document.location.pathname)) {
  s.events = "event5";
}

{% analytics_tag landing %}s.events ="prodView,event1";{% endanalytics_tag %}
{% analytics_tag signup %}s.events = "event13";{% endanalytics_tag %}
{% analytics_tag sale %}
s.events     = "purchase";
s.products   = ";{% analytics_tag_data item_id %};{% analytics_tag_data quantity %};{% analytics_tag_data value %}";
s.purchaseID = "{% analytics_tag_data sale_id %}";
s.price = "{{ daily_deal.price }}";
{% endanalytics_tag %}

s.prop6 = "twc";
{% show_twc_logo %}s.prop6 = "rr";{% endshow_twc_logo %}

// Whether it was a normal registration or a Facebook connect registration.
// s.eVar7  = "";
//*** End TWC Omniture Variables ***/
</script>

<!-- Add knotice tracking tags to all pages -->
<script type="text/javascript">
    if (typeof (__kcv) === 'undefined') {
        __kcv = { w: 0, o: '', a: '', h: 'deals.clickedin.com', t:false,e:3 }; 	
    }	
    {% analytics_tag sale %}
        __kcv.PRICE = "{{ daily_deal.price }}";
    {% endanalytics_tag %}
</script>

<script type="text/javascript">

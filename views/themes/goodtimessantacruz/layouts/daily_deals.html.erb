<!--START DAILY DEALS CODE-->

<div id="analog_analytics">
  <div id="daily_deal">

  	<div id="outer_frame">
      	<div id="inner_frame">

                      <div id="header_nav">
                          <div id="promo">&nbsp;</div>
                          <div id="nav">
                            <%= link_to("Today's Deal", public_deal_of_day_path(@publisher.label)) %> 
                            <%= link_to("Contact Us", contact_publisher_daily_deals_path(@publisher))%>  
                            <%= link_to("FAQ", faqs_publisher_daily_deals_path(@publisher))%>  
                            <% if current_consumer_for_publisher?(@publisher) -%>
                              <%= link_to("My Account", publisher_consumer_daily_deal_purchases_path(@publisher, current_consumer)) %>
                              <%= link_to("Sign Out", daily_deal_logout_path(@publisher)) %>
                            <% else -%>
                              <%= link_to("Sign In", daily_deal_login_path(@publisher), :style => "border:none;") %>
                            <% end -%>
                          </div>
                      </div>

              <div id="dd_content">
      			<div id="inner_content">
                  	<div id="deal_of_the_day_banner">
                              <div id="dd_logo"><%= link_to( image_tag("/themes/goodtimessantacruz/images/display/deal_banner.png", :alt => "Deal of the Day", :width => "575", :height => "100"), public_deal_of_day_path(@publisher.label) )%></a></div>
            <div id="email">
                               <% form_for :subscriber, @subscriber, :url => publisher_subscribers_path(@publisher), :html => {:id => "daily_deal_header", :class => "email_subscription_tracking"} do |f| -%>
                                 <%= f.hidden_field :must_accept_terms, :value => true %>
                                 <%= hidden_field_tag :redirect_to, request.url %>
                                   <div class="maincontent">
                                     <%= f.text_field :email, :class => "dd_email_input ghostie", :value => "Enter Email", :title => "Enter Email" %>
                                     <input type="image" id="submit_btn" class="sign_up_btn_form" src="/themes/goodtimessantacruz/images/buttons/subscribe.png" />
                                   </div>
                                   <div class="terms">
                                    <%= f.check_box :terms, :class => "terms_agree" %> I agree to the <%= link_to("Terms", terms_publisher_daily_deals_path(@publisher) )%> and <%= link_to("Privacy Policy", privacy_policy_publisher_daily_deals_path(@publisher)) %>
                                   </div>
                                   
                              <% end -%>
                              
                           
          </div>
                      </div> 
                      <% if flash.present? -%>
                      <div class="flash">
                        <div class="warn" id="flash_warn" style="display: <%= flash[:warn].present? ? "visible" : "none" %>;">
                          <%= flash[:warn] %>
                        </div>
                        <div class="notice" id="flash_notice" style="display: <%= flash[:notice].present? ? "visible" : "none" %>;">
                          <%= flash[:notice] %>
                        </div>  
                      </div>                     
                      <% end -%>
                      <%= yield %>
                              
                               <% if @daily_deal.present? -%>
                               <div id="side_bar">
                               <div id="publisher"><img src="/themes/goodtimessantacruz/images/display/publisher.png" width="230" height="89" alt="publisher" /></div>

                                   <div id="side_content_box">
                                       <h2>To Our Readers</h2>
                                       <p>Thank you for checking out Good Times Santa Cruz's Deal of the Day! We're working hard to bring you big savings from Santa Cruz's most trusted merchants, including everything from restaurants to spas to events. We hope you'll sign up for our email list and check back often so you won't miss any great deals! Have a great time with Good Times' Deal of the Day!</p>
                                   </div>

                                  <div id="side_content_box">
                                       <h2>Feature Your Business</h2>
                                       <p>Contact our Sales Manager</p>
                                       <%= link_to( image_tag("/themes/goodtimessantacruz/images/buttons/click_here_button.png"), "mailto:stephanie@gtweekly.com", :width => 81, :height => 21 ) %>
                                   </div>
                                   
                                   <% if @daily_deal.present? -%>
                                   <div id="side_content_box">
                                       <h2>MAP &amp; Location</h2>
                                       <% if @daily_deal.advertiser.map_url.present? && !@daily_deal.multi_location? %>
                                       <%= link_to(image_tag(map_image_url_for(@daily_deal, "199x169"), :id => "map"), @daily_deal.advertiser.map_url, :target => "_blank") %>
                                       <% end -%>
                                  <p><%= render :partial => "/themes/goodtimessantacruz/daily_deals/address" %></p>
                                   </div>
                                   <% end -%>


                               </div>
                               <% end -%>
                  </div>
                  <div class="clear">&nbsp;</div>
      		</div>

      	</div>
      </div> 



  </div>    

</div>


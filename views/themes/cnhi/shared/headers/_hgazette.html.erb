function display_ad(ads)
{
  if(ads == '')
  {
    document.write('<!-- No ads in feed for this position in this section -->');
    return;
  }else{  
  	if(ads.length > 0)
  	{
  		var tmp_ads = Array();
  		for (var ad in ads)
  		{
  			var currad = ads[ad];
  			for (var x = 0; x < currad.shares; x++)
  			{
  				tmp_ads.push(currad);
  			}
  		}
  		var ad = tmp_ads[Math.floor(Math.random()*(tmp_ads.length))];
  		var code = html_entity_decode(ad.code);
  		if(ad.type == 'GOO')
  		{
  			eval(code);
  		}else{
  			document.write(code);
  		}
  		if(ad.ad_event)
  		{
  			adTracker._trackEvent('AdLoad', ad.ad_event);
  		}
  	}
  }
}

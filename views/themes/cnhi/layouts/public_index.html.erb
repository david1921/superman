  <div id="masthead"> 
    <h1 id="logo"> 
      <a accesskey="1" href="http://<%= @publisher.production_host.gsub("coupons.", "") %>"
         title="<%= @publisher.name %>"><img src='/themes/cnhi/images/logos/<%=@publisher.label%>_logo.jpg' alt='<%= @publisher.name %>' /></a> 
    </h1> 
    <div id="leaderboard" class="noprint"> 

<% unless @publisher.label == "cnhi-itzmag" %>
<script type="text/javascript"> 
SOVA.fill('leaderboard');
</script> 
<% end %>
 
    </div> 
  </div> 
  <div id="pencil_ad"> 
</div> 
  
  <div id="highlights"> 
    
    
  </div> 
  
  <div id="content"> 
  
    
    
      <h2 id="section_header"> 
        
        <a href="#"><% case @publisher.label 
						when "cnhi-thejaspernews" -%>

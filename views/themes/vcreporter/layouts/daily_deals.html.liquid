<!--START DAILY DEALS CODE-->

<div id="client_header">
  {% include "themes/vcreporter/shared/header" %}
</div>

<div id="analog_analytics">
    <div id="daily_deal">
    
        <div id="outer_frame">
            <div id="inner_frame">
            
                <div id="header_nav">
                	<div id="promo">
                	  {% if publisher.show_special_deal? %}
                	  <a href="{{ publisher.deal_credit_path }}">Special Promo: Sign up here to receive your $10 credit</a>
                	  {% endif %}
                	</div>
                  <div id="nav">
                    <a href="{{ publisher.todays_daily_deal_path }}">Today's Deal</a> 
                    <a href="{{ publisher.contact_path }}">Contact Us</a>  
                    <a href="{{ publisher.faq_path }}">FAQ</a>
                    {% if publisher.current_user_belongs_to? %}
                    <a href="{{ publisher.my_deals_path }}">My Deals</a>
                    <a href="{{ publisher.my_account_path }}">My Account</a>
                    <a class="no_border" href="{{ publisher.logout_path }}">Sign Out</a>
                    {% else %}
                    <a class="no_border" href="{{ publisher.login_path }}">Sign In</a>
                    {% endif %}
                  </div>
                </div>
            
                <div id="dd_content">
                    <div id="inner_content">
                        <div id="deal_of_the_day_banner"><!--MASTHEAD-->
                        	<div id="dd_logo"><a href="{{ publisher.todays_daily_deal_path }}"><img src="/themes/vcreporter/images/display/deal_banner.png" alt="Deal of the Day" width="575" height="100" /></a></div>
                            <div id="email">
                                <form action="{{ publisher.subscribers_path }}" id="daily_deal_header" class="email_subscription_tracking" method="post">
                                  <input id="redirect_to" name="redirect_to" type="hidden" value="{{ publisher.subscribers_thank_you_path | verifiable_url }}" />
                                    <div class="maincontent">
                                    	<input id="subscriber_email" name="subscriber[email]" size="27" type="text">
                                    	<input id="submit_btn" src="/themes/vcreporter/images/buttons/subscribe.png" type="image">
                                    </div>
                                    <div class="terms">
                                    	<input name="subscriber[terms]" value="0" type="hidden">
                                      <input type="hidden" value="1" name="subscriber[must_accept_terms]">
                                    	<input id="subscriber_terms" name="subscriber[terms]" value="1" type="checkbox"> I agree to the &nbsp;<a href="#" onclick="window.open('{{ daily_deal.terms_url }}', '_blank'); return false; return false;">Terms</a> and &nbsp;<a href="#" onclick="window.open('{{ daily_deal.privacy_policy_url }}', '_blank'); return false; return false;">Privacy Policy</a>
                                    </div>                                
                                </form>
                            </div>
                        </div><!--/MASTHEAD-->
                        
                    <div id="main"><!--MAIN CONTENT-->
                      {% include "shared/flash" %}
                      {{ content_for_layout }}
                   </div><!--/MAIN CONTENT-->
                </div>
                
                    <div class="clear">
                    </div>
                    
                </div>
            
            </div>
        </div> 
        
      <div>
        {% include "themes/vcreporter/shared/footer" %}
      </div>
     {% include "shared/a_a_alt" %}
        
       
    </div>

</div>


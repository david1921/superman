<!-- facebook tab page script to get rid of the scroll bars -->
<script type="text/javascript">
window.fbAsyncInit = function() {
FB.Canvas.setSize();
}

// Do things that will sometimes call sizeChangeCallback()
function sizeChangeCallback() {
FB.Canvas.setSize();
}
</script>
<!-- END facebook tab page script to get rid of the scroll bars -->  
  
  <title>{% daily_deal_page_title %}</title>
{% include "shared/tracking_google_analytics" %}
</head>

<body class="fbtab">



<div class="clear"></div>
<!--Analog_Analytics_Daily_Deal-->


<div id="analog_analytics">
	<a href="{{ publisher.todays_daily_deal_path }}" target="_blank">{% include with_theme "shared/banner" %}</a>
	{% include with_theme "shared/dd_nav_fb" %}

	<div id="daily_deal">
	
		 
		<div id="dd_content">
	       	 
		    {% include "shared/flash" %}
		    {{ content_for_layout }}
		    <div class="clear"></div>
		    {% include "shared/a_a" %}
		</div><!--dd_content-->
		
		<div id="dd_footer">
			<ul id="footer_nav">
				<li><a href="{{ publisher.todays_daily_deal_path }}" target="_blank">Today's Deal</a> </li>
				<li><a href="{{ publisher.how_it_works_path }}" target="_blank">How It Works</a></li>
				<!-- <li><a href="">Past Deals</a></li> -->
				<li><a href="{{ publisher.feature_your_business_path }}" target="_blank">Feature Your Business</a></li>
				<li><a href="{{ publisher.faq_path }}" target="_blank">FAQ</a></li>
				<li class="last"><a href="{{ publisher.contact_path }}" target="_blank">Contact Us</a></li>
			</ul><!-- /footer_nav -->
			<div class="clear"></div>
			<ul id="legal_nav">
				<li><a href="{{ publisher.privacy_path }}" target="_blank">Privacy Policy</a></li>
				<li class="last"><a href="{{ publisher.terms_path }}" target="_blank">Terms and Conditions</a></li>
			</ul><!-- /legal_nav -->
		</div><!-- /dd_footer -->
	</div><!--daily_deal-->

</div><!--analog_analytics-->

<!-- facebook tab script to get rid of scroll bars -->
<div id="fb-root"></div>
<script src="http://connect.facebook.net/en_US/all.js"></script>
<script>
FB.init({
appId : '211242462221554',
status : true, // check login status
cookie : true, // enable cookies to allow the server to access the session
xfbml : true // parse XFBML
});
</script>
<!-- END facebook tab script to get rid of scroll bars -->
{% include with_theme "shared/tracking_publisher" %}
{% include "daily_deals/javascript" %}
</body>

<?xml version="1.0" encoding="UTF-8"?>
 
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
  xmlns:go="http://base.google.com/ns/1.0"
  targetNamespace="http://base.google.com/ns/1.0"
  elementFormDefault="qualified" attributeFormDefault="unqualified">
<xs:annotation>
  <xs:documentation>
    The Google Offers Feed is available for public use by authorized merchants.
    NOTE: The words 'merchant', 'partner', 'aggregator', 'deal provider', etc. are used
          inter-changeably to indicate the single point of contact for Google.
    VERSION: All modifications to the feed spec will be backward-compatible, and
             always result in an increase of the major/minor version number.
  </xs:documentation>
</xs:annotation>
 
  <xs:import namespace="http://www.w3.org/XML/1998/namespace"
    schemaLocation="http://www.w3.org/2001/03/xml.xsd"/>
 
  <xs:element name="feed">
    <xs:annotation>
      <xs:documentation>
        The offers feed supplied by the merchant to Google.
      </xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="go:entry" maxOccurs="unbounded"/>
      </xs:sequence>
      <xs:attribute ref="xml:lang" use="required"/>
    </xs:complexType>
  </xs:element>
 
  <xs:element name="entry">
    <xs:annotation>
      <xs:documentation>
        The individual offers supplied by the deal provider to Google.
      </xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="go:id"/>
        <xs:element ref="go:type"/>
        <xs:element ref="go:title"/>
        <xs:element ref="go:short_title" minOccurs="0"/>
        <xs:element ref="go:description"/>
        <xs:element ref="go:display_image"/>
        <xs:element ref="go:attribution_image"/>
        <xs:element ref="go:highlight" minOccurs="0" maxOccurs="10"/>
        <xs:element ref="go:fine_print" minOccurs="0" maxOccurs="10"/>
 
        <xs:element ref="go:original_price"/>
        <xs:element ref="go:offer_price"/>
        <xs:element ref="go:max_available"/>
        <xs:element ref="go:max_allow_user"/>
        <xs:element ref="go:distribution_date"/>
        <xs:element ref="go:redemption_date"/>
 
        <xs:element ref="go:merchant"/>
        <xs:element ref="go:merchant_location"/>
        <xs:element ref="go:redemption"/>
        <xs:element ref="go:redemption_location" minOccurs="0" maxOccurs="10000"/>
        <xs:element ref="go:redemption_code" minOccurs="0" maxOccurs="unbounded"/>
 
        <xs:element ref="go:applicable_metro_code" minOccurs="0" maxOccurs="100"/>
        <xs:element ref="go:applicable_province_code" minOccurs="0" maxOccurs="50"/>
        <xs:element ref="go:applicable_country_code" minOccurs="0" maxOccurs="25"/>
        <xs:element ref="go:tag" minOccurs="0" maxOccurs="10"/>
        <xs:element ref="go:category" minOccurs="0" maxOccurs="10"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
 
  <xs:element name="id" type="go:idType">
    <xs:annotation>
      <xs:documentation>
        The offer id supplied by the deal provider, must be unique across all their offers.
      </xs:documentation>
    </xs:annotation>
  </xs:element>
 
  <xs:element name="type" type="go:offerType">
    <xs:annotation>
      <xs:documentation>
        The type of offer - note that each type has distinct characteristics.
      </xs:documentation>
    </xs:annotation>
  </xs:element>
 
  <xs:element name="title" type="go:shortTextType">
    <xs:annotation>
      <xs:documentation>
        The title displayed on the offer landing page on Google Offers.
      </xs:documentation>
    </xs:annotation>
  </xs:element>
 
  <xs:element name="short_title" type="go:shortTextType">
    <xs:annotation>
      <xs:documentation>
        The title displayed on the offer snippet view on Google Offers.
      </xs:documentation>
    </xs:annotation>
  </xs:element>
 
  <xs:element name="description" type="go:longTextType">
    <xs:annotation>
      <xs:documentation>
        A detailed description of the offer.
      </xs:documentation>
    </xs:annotation>
  </xs:element>
 
  <xs:element name="display_image" type="go:linkType">
    <xs:annotation>
      <xs:documentation>
        The image displayed with the offer. It must be in 'png' format (4:3 aspect ratio).
      </xs:documentation>
    </xs:annotation>
  </xs:element>
 
  <xs:element name="attribution_image" type="go:linkType">
    <xs:annotation>
      <xs:documentation>
        The image displayed with the offer, purely for external attribution. It must be in
        'png' format with a transparent background (4:1 aspect ratio).
      </xs:documentation>
    </xs:annotation>
  </xs:element>
 
  <xs:element name="highlight" type="go:shortTextType">
    <xs:annotation>
      <xs:documentation>
        The list of attractive attributes of the offer.
      </xs:documentation>
    </xs:annotation>
  </xs:element>
 
  <xs:element name="fine_print" type="go:shortTextType">
    <xs:annotation>
      <xs:documentation>
        The list of terms and conditions for the offer.
      </xs:documentation>
    </xs:annotation>
  </xs:element>
 
  <xs:element name="original_price" type="go:priceType">
    <xs:annotation>
      <xs:documentation>
        The original item price.
      </xs:documentation>
    </xs:annotation>
  </xs:element>
 
  <xs:element name="offer_price" type="go:priceType">
    <xs:annotation>
      <xs:documentation>
        The discounted item price.
      </xs:documentation>
    </xs:annotation>
  </xs:element>
 
  <xs:element name="max_available" type="xs:nonNegativeInteger">
    <xs:annotation>
      <xs:documentation>
        The maximum number of offers that can be sold.
      </xs:documentation>
    </xs:annotation>
  </xs:element>
 
  <xs:element name="max_allow_user" type="xs:nonNegativeInteger">
    <xs:annotation>
      <xs:documentation>
        The maximum number of offers sold to a single user.
      </xs:documentation>
    </xs:annotation>
  </xs:element>
 
  <xs:element name="distribution_date" type="go:timeRangeType">
    <xs:annotation>
      <xs:documentation>
        The date range during which the offer can be distributed.
      </xs:documentation>
    </xs:annotation>
  </xs:element>
 
  <xs:element name="redemption_date" type="go:timeRangeType">
    <xs:annotation>
      <xs:documentation>
        The date range during which the offer can be redeemed.
      </xs:documentation>
    </xs:annotation>
  </xs:element>
 
  <xs:element name="merchant">
    <xs:annotation>
      <xs:documentation>
        The actual merchant who is supplying this offer.
      </xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element name="name" type="xs:string">
          <xs:annotation>
            <xs:documentation>
              The user visible name of the merchant.
            </xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="description" type="go:longTextType">
          <xs:annotation>
            <xs:documentation>
              A detailed description of the offer.
            </xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="url" type="go:linkType" minOccurs="0">
          <xs:annotation>
            <xs:documentation>
              The merchant website for direct end-user access.
            </xs:documentation>
          </xs:annotation>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
 
  <xs:element name="merchant_location" type="go:contactType">
    <xs:annotation>
      <xs:documentation>
        The detailed info about location/contact details for the merchant.
      </xs:documentation>
    </xs:annotation>
  </xs:element>
 
  <xs:element name="redemption">
    <xs:annotation>
      <xs:documentation>
        The redemption specific details for the offer.
      </xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element name="code_provider_type" type="go:codeProviderType">
          <xs:annotation>
            <xs:documentation>
              The entity that generates the redemption codes for each offer sold.
            </xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="code_display_type" type="go:codeDisplayType">
          <xs:annotation>
            <xs:documentation>
            </xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="instructions" type="go:longTextType" minOccurs="0">
          <xs:annotation>
            <xs:documentation>
              Any specific redemption instructions to be followed by the user for this offer.
            </xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="online_redemption_url" type="go:linkType" minOccurs="0">
          <xs:annotation>
            <xs:documentation>
              If the offer is available online, specify the url where it can be redeemed.
            </xs:documentation>
          </xs:annotation>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
 
  <xs:element name="redemption_location" type="go:contactType">
    <xs:annotation>
      <xs:documentation>
        The list of stores where the offer is redeemable.
        If unspecified, we assume that the offer is not available in any store.
      </xs:documentation>
    </xs:annotation>
  </xs:element>
 
  <xs:element name="redemption_code" type="go:idType">
    <xs:annotation>
      <xs:documentation>
        The list of redemption codes to be used for each offer sold.
        It is incorrect to provide them here when code_provider_type is not of merchant_feed type.
      </xs:documentation>
    </xs:annotation>
  </xs:element>
 
  <xs:element name="applicable_metro_code" type="go:metroCodeType">
    <xs:annotation>
      <xs:documentation>
        The US metros to be targeted for the distribution of this offer.
      </xs:documentation>
    </xs:annotation>
  </xs:element>
 
  <xs:element name="applicable_province_code" type="go:provinceCodeType">
    <xs:annotation>
      <xs:documentation>
        The international provinces to be targeted for the distribution of this offer.
      </xs:documentation>
    </xs:annotation>
  </xs:element>
 
  <xs:element name="applicable_country_code" type="go:countryCodeType">
    <xs:annotation>
      <xs:documentation>
        The countries to be targeted for the distribution of this offer.
      </xs:documentation>
    </xs:annotation>
  </xs:element>
 
  <xs:element name="tag" type="go:shortTextType">
    <xs:annotation>
      <xs:documentation>
        The list of free-form keywords/tags to help better represent/categorize the offer.
      </xs:documentation>
    </xs:annotation>
  </xs:element>
 
  <xs:element name="category" type="go:categoryType">
    <xs:annotation>
      <xs:documentation>
        The list of structured categories to help better represent the offer.
      </xs:documentation>
    </xs:annotation>
  </xs:element>
 
  <xs:simpleType name="idType">
    <xs:annotation>
      <xs:documentation>
        The custom data type used to represent the different identifiers in this feed.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:pattern value="[0-9a-zA-Z\-_]+"/>
    </xs:restriction>
  </xs:simpleType>
 
  <xs:simpleType name="shortTextType">
    <xs:annotation>
      <xs:documentation>
        The custom data type used to represent a short title text.
        No HTML content is permitted here.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:maxLength value="100"/>
    </xs:restriction>
  </xs:simpleType>
 
  <xs:simpleType name="longTextType">
    <xs:annotation>
      <xs:documentation>
        The custom data type used to represent a long descriptive text.
        Only a few HTML tags are permitted here - namely br, b, i.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:maxLength value="10000"/>
    </xs:restriction>
  </xs:simpleType>
 
  <xs:complexType name="linkType">
    <xs:annotation>
      <xs:documentation>
        The custom data type used to represent links/urls.
      </xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base="xs:string">
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
 
  <xs:complexType name="priceType">
    <xs:annotation>
      <xs:documentation>
        The custom data type used to represent price in different currencies.
        Format: "price currency", where
                  'price' is in xs:decimal format,
                  'currency' is in go:currencyCodeType format,
                  ' ' is the delimiter.
      </xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base="xs:string">
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
 
  <xs:complexType name="timeRangeType">
    <xs:annotation>
      <xs:documentation>
        The custom data type used to represent date ranges.
        Format: "start_date/end_date", where
                  'start_date' is in xs:dateTime format,
                  'end_date' is in xs:dateTime format,
                  '/' is the delimiter.
      </xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base="xs:string">
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
 
  <xs:complexType name="contactType">
    <xs:annotation>
      <xs:documentation>
        The custom data type used to represent location/contact information.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="address_1" type="xs:string">
        <xs:annotation>
          <xs:documentation>
            The postal address of the merchant/store - line 1.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="address_2" type="xs:string" minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            The postal address of the merchant/store - line 2.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="address_3" type="xs:string" minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            The postal address of the merchant/store - line 3.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="city" type="xs:string">
        <xs:annotation>
          <xs:documentation>
            The city where the merchant/store is located.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="state" type="xs:string">
        <xs:annotation>
          <xs:documentation>
            The state where the merchant/store is located.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="postal_code" type="xs:string">
        <xs:annotation>
          <xs:documentation>
            The zipcode of the merchant/store location.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="country_code" type="go:countryCodeType">
        <xs:annotation>
          <xs:documentation>
            The appropriate country code from the official list provided here.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="phone" type="xs:string" minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            The customer service phone number of the merchant/store.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="email" type="xs:string" minOccurs="0">
      <xs:annotation>
        <xs:documentation>
          The customer service email of the merchant/store.
        </xs:documentation>
      </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
 
  <xs:simpleType name="codeProviderType">
    <xs:annotation>
      <xs:documentation>
        The custom data type used to represent the entity that generates redemption codes.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="google">
        <xs:annotation>
          <xs:documentation>
            The redemption codes would be auto-generated by Google.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="merchant_feed">
        <xs:annotation>
          <xs:documentation>
            All redemption codes are provided by the merchant as part of this feed entry.
            No redemption codes are dynamically pushed to Google using the Google Offers API.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="merchant_api">
        <xs:annotation>
          <xs:documentation>
            No redemption codes are provided by the merchant as part of this feed entry.
            All redemption codes are dynamically pushed to Google using the Google Offers API.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
 
  <xs:simpleType name="codeDisplayType">
    <xs:annotation>
      <xs:documentation>
        The custom data type used to represent the display format of readily scannable content.
        See http://en.wikipedia.org/wiki/Barcode for more details.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:annotation>
        <xs:documentation>
          The content does not need any fancy formatting, simple text display is sufficient.
        </xs:documentation>
      </xs:annotation>
      <xs:enumeration value="alphanumeric"/>
      <xs:enumeration value="ean_8"/>
      <xs:enumeration value="ean_13"/>
      <xs:enumeration value="code_39"/>
      <xs:enumeration value="code_128"/>
      <xs:enumeration value="itf_14"/>
      <xs:enumeration value="qrcode"/>
    </xs:restriction>
  </xs:simpleType>
 
  <xs:simpleType name="categoryType">
    <xs:annotation>
      <xs:documentation>
        The list of structured categories to help better represent the offer.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="GLAMOUR_SERVICES">
        <xs:annotation>
          <xs:documentation>
            Description: haircuts and color, waxing, nails and other salon or barber services
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="RELAXATION_SERVICES">
        <xs:annotation>
          <xs:documentation>
            Description: massage, facials, body treatments and other relaxing day spa services
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="MEDICAL_COSMETIC_SERVICES">
        <xs:annotation>
          <xs:documentation>
            Description: laser hair removal, injectables, cellulite treatments and
                         other self-renovation services
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="EXERCISE_ACTIVITIES">
        <xs:annotation>
          <xs:documentation>
            Description: personal training, fitness classes, yoga and other healthy activities
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="ADVENTURE_ACTIVITIES">
        <xs:annotation>
          <xs:documentation>
            Description: sailing, horseback riding, paintball and other action-packed activities
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="HEALTHFUL_SERVICES">
        <xs:annotation>
          <xs:documentation>
            Description: vision care, dental services, weight loss services and other health help
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="TRIPS_AND_GETAWAYS">
        <xs:annotation>
          <xs:documentation>
            Description: vacation packages, hotels, transportation and other travel services
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="LOCAL_TOURS_AND_SIGHTS">
        <xs:annotation>
          <xs:documentation>
            Description: tours, sightseeing, theme parks and other local activities
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="ARTS_AND_CULTURE_ACTIVITIES">
        <xs:annotation>
          <xs:documentation>
            Description: museums, performing arts, film festivals and other artsy activities
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="SPORTS_AND_ENTERTAINMENT_EVENTS">
        <xs:annotation>
          <xs:documentation>
            Description: concerts, nightlife, sports events and other entertaining activities
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="CREATIVE_CLASSES">
        <xs:annotation>
          <xs:documentation>
            Description: cooking classes, art, music lessons and other creative life skills
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="HOME_AND_GARDEN_SERVICES">
        <xs:annotation>
          <xs:documentation>
            Description: carpet cleaning, home repair, landscaping and other helpful services
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="PROFESSIONAL_EXPERT_SERVICES">
        <xs:annotation>
          <xs:documentation>
            Description: tutoring, tax prep and other expert services
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="AUTO_CAR_CARE">
        <xs:annotation>
          <xs:documentation>
            Description: auto repair, oil change, car detailing and other car care
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="RETAIL_SHOPPING">
        <xs:annotation>
          <xs:documentation>
            Description: gadgets, home decor, fashion and other retail therapy
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="FAMILY_KIDS_FUN">
        <xs:annotation>
          <xs:documentation>
            Description: classes for kids, trips to the zoo, children's museums and
                         other family friendly fun
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="PERSONAL_COMFORT_SERVICES">
        <xs:annotation>
          <xs:documentation>
            Description: personal shoppers, event planners, dog walkers and other personal services
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="EATING_AND_DRINKING">
        <xs:annotation>
          <xs:documentation>
            Description: restaurants, wine tastings, gourmet goods and other eatables
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
 
  <xs:simpleType name="offerType">
    <xs:annotation>
      <xs:documentation>
        The type of offer - note that each type has distinct characteristics.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:annotation>
        <xs:documentation>
          A pre-paid offer sourced externally by Google.
        </xs:documentation>
      </xs:annotation>
      <xs:enumeration value="marketplace_prepaid"/>
    </xs:restriction>
  </xs:simpleType>
 
